{
	"name": "react-template-project",
	"author": "Bryan Aguilar",
	"description": "React template project with: Vite, Typescript, ESLint, Prettier, SCSS, Commitlint, Husky, alias paths, Jest & React Testing Library",
	"private": true,
	"version": "0.1.0",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=18.12.1",
		"yarn": ">=1.22.0",
		"npm": "please-use-yarn"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx,scss,md,json} --config ./.prettierrc",
		"prepare": "husky install",
		"test": "jest --watchAll",
		"test:coverage": "jest --coverage"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@tanstack/react-query": "^5.62.7",
		"@trpc/client": "^11.0.0-rc.446",
		"@trpc/react-query": "^11.0.0-rc.446",
		"@trpc/server": "^11.0.0-rc.446",
		"classnames": "^2.5.1",
		"framer-motion": "^11.13.5",
		"html2canvas": "^1.4.1",
		"jspdf": "^2.5.2",
		"lodash-es": "^4.17.21",
		"next": "^15.1.0",
		"pusher-js": "^8.4.0-rc2",
		"radix-ui": "^1.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.54.0",
		"react-icons": "^5.4.0",
		"react-resizable-panels": "^2.1.7",
		"react-tooltip": "^5.28.0",
		"sonner": "^1.7.1",
		"superjson": "^2.2.2",
		"uuid": "^11.0.3",
		"vite-tsconfig-paths": "^5.1.4",
		"zustand": "^5.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"babel-jest": "^29.3.1",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"isomorphic-fetch": "^3.0.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"prettier": "^2.8.1",
		"sass": "^1.57.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	}
}
